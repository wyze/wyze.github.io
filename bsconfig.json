{
  "name": "wyze.github.io",
  "version": "1.0.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": [
        {
          "dir": "components",
          "subdirs": [
            {
              "dir": "__tests__",
              "type": "dev"
            }
          ]
        },
        {
          "dir": "pages",
          "subdirs": [
            {
              "dir": "__tests__",
              "type": "dev"
            }
          ]
        },
        "config",
        "utils"
      ]
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bsc-flags": [
    "-bs-no-version-header",
    "-bs-super-errors"
  ],
  "reason": {
    "react-jsx": 2
  },
  "bs-dependencies": [
    "@ahrefs/bs-dotenv",
    "@wyze/bs-react-helmet",
    "bs-css",
    "bs-intersection-observer",
    "reason-react"
  ],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest",
    "bs-jest-dom",
    "bs-react-testing-library"
  ],
  "refmt": 3
}
