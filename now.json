{
  "alias": "neilkistner.com",
  "build": {
    "env": {
      "GITHUB_TOKEN": "@github-token"
    }
  },
  "builds": [
    { "src": "analytics/Cargo.toml", "use": "now-rust" },
    { "src": "package.json", "use": "@now/static-build" }
  ],
  "public": true,
  "routes": [
    {
      "src": "/.*\\.(ico|js|json|png|woff2)",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "src": "/images/(.*)", "dest": "/analytics/$1"
    }
  ],
  "version": 2
}
