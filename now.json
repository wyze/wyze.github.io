{
  "alias": "neilkistner.com",
  "build": {
    "env": {
      "GITHUB_TOKEN": "@github-token"
    }
  },
  "functions": {
    "api/**/*.rs": {
      "runtime": "now-rust@1.0.0"
    }
  },
  "public": true,
  "routes": [
    {
      "src": "/.*\\.(ico|js|json|png|woff2)",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    }
  ],
  "version": 2
}
