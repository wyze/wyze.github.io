{
  "name": "wyze.github.io",
  "private": true,
  "version": "0.0.0",
  "repository": "wyze/wyze.github.io",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "https://neilkistner.com"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn build:config",
    "build": "next build",
    "build:config": "rio -e next-offline -o next.config.js next.config.ts",
    "predev": "yarn build:config",
    "dev": "next",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prestart": "yarn build:config",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@octokit/graphql": "5.0.1",
    "next": "12.3.1",
    "otion": "0.6.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-otion": "0.6.2"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.3",
    "@babel/preset-typescript": "7.18.6",
    "@next/eslint-plugin-next": "12.3.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@typescript-eslint/eslint-plugin": "5.39.0",
    "@typescript-eslint/parser": "5.39.0",
    "@wyze/rio": "1.4.0",
    "babel-jest": "29.1.2",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.1.1",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.7.2",
    "eslint-plugin-wyze": "3.4.0",
    "jest": "29.1.2",
    "next-offline": "5.0.5",
    "prettier": "2.7.1",
    "typescript": "4.8.4"
  },
  "resolutions": {
    "glob-parent": "5.1.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/test-utils.tsx"
    ],
    "moduleNameMapper": {
      "\\.(css|pdf|png(\\?webp)?)$": "<rootDir>/__mocks__/file.ts"
    },
    "roots": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ],
    "testEnvironment": "jsdom"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
