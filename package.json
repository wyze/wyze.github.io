{
  "name": "wyze.github.io",
  "private": true,
  "version": "0.0.0",
  "repository": "wyze/wyze.github.io",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "https://neilkistner.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint src/**/*.{ts,tsx}",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "scripts_": {
    "build": "run-s build:*",
    "build:bsb": "bsb -make-world",
    "build:config": "rio -o gatsby-config.js src/config/gatsby.re",
    "build:gatsby": "gatsby build",
    "clean": "bsb -clean-world && rimraf .cache .merlin lib public *.js static/js",
    "dev": "run-p dev:*",
    "dev:bsb": "yarn build:bsb -w",
    "dev:gatsby": "gatsby develop",
    "jest": "jest",
    "prejest": "rio -o babel.config.js src/config/babel.re",
    "predev:gatsby": "yarn build:config",
    "pretest": "yarn build:bsb",
    "test": "yarn jest"
  },
  "dependencies": {
    "@octokit/graphql": "^4.3.1",
    "fela": "^11.2.0",
    "fela-dom": "^11.2.0",
    "fela-plugin-named-keys": "^11.2.0",
    "fela-plugin-typescript": "^11.2.0",
    "fela-preset-web": "^11.2.0",
    "fela-sort-media-query-mobile-first": "^11.2.0",
    "next": "^9.3.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fela": "^11.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^25.2.0",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-jest": "^25.2.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jest-dom": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-testing-library": "^3.0.2",
    "eslint-plugin-wyze": "^3.4.0",
    "jest": "^25.2.7",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.0",
    "next-optimized-images": "^2.5.7",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3",
    "webp-loader": "^0.6.0"
  },
  "dependencies_": {
    "@wyze/bs-react-helmet": "^0.3.1",
    "bs-css": "^7.2.0",
    "bs-intersection-observer": "^0.2.0",
    "gatsby": "^2.15.28",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-helmet": "^5.2.0",
    "reason-react": "^0.5.3"
  },
  "devDependencies_": {
    "@ahrefs/bs-dotenv": "^1.0.3",
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@glennsl/bs-jest": "^0.4.4",
    "@wyze/gatsby-plugin": "^1.0.0",
    "@wyze/gatsby-source-graphql": "^1.2.0",
    "@wyze/rio": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "bs-jest-dom": "^1.0.0",
    "bs-platform": "^4.0.5",
    "bs-react-testing-library": "^0.4.0",
    "dotenv": "^6.1.0",
    "gatsby-plugin-favicon": "^3.1.2",
    "gatsby-plugin-glamor": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.6",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-source-filesystem": "^2.0.3",
    "identity-obj-proxy": "^3.0.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2"
  },
  "resolutions_": {
    "reason-react": "^0.5.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(pdf|png(\\?webp)?)$": "<rootDir>/__mocks__/file.ts"
    },
    "roots": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "babel": {
    "env": {
      "tes": {
        "presets": [
          [
            "@babel/env",
            {
              "targets": {
                "node": "current"
              }
            }
          ],
          "@babel/typescript"
        ]
      }
    },
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "jest_": {
    "collectCoverageFrom": [
      "**/{components,pages}/**/*.bs.js"
    ],
    "moduleNameMapper": {
      "^.+\\.png$": "identity-obj-proxy"
    },
    "roots": [
      "src"
    ],
    "setupFiles": [
      "./src/config/setupTests.bs.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@ahrefs/bs-dotenv|@glennsl/bs-jest|@wyze/bs-react-helmet|bs-|reason-react))"
    ]
  }
}
