{
  "name": "wyze.github.io",
  "private": true,
  "version": "0.0.0",
  "repository": "wyze/wyze.github.io",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "https://neilkistner.com"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn build:config",
    "build": "next build",
    "build:config": "rio -e next-offline,next-optimized-images,next-compose-plugins -o next.config.js next.config.ts",
    "predev": "yarn build:config",
    "dev": "next",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prestart": "yarn build:config",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@octokit/graphql": "^4.5.6",
    "fela": "^11.3.3",
    "fela-dom": "^11.3.3",
    "fela-plugin-named-keys": "^11.3.3",
    "fela-plugin-typescript": "^11.3.3",
    "fela-preset-web": "^11.3.3",
    "fela-sort-media-query-mobile-first": "^11.3.3",
    "next": "^9.5.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-fela": "^11.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@wyze/rio": "^1.4.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-testing-library": "^3.9.0",
    "eslint-plugin-wyze": "^3.4.0",
    "jest": "^26.4.2",
    "next-compose-plugins": "^2.2.0",
    "next-offline": "^5.0.2",
    "next-optimized-images": "^2.6.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "webp-loader": "^0.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/test-utils.tsx"
    ],
    "moduleNameMapper": {
      "\\.(pdf|png(\\?webp)?)$": "<rootDir>/__mocks__/file.ts"
    },
    "roots": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
