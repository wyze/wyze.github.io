{
  "name": "wyze.github.io",
  "private": true,
  "version": "0.0.0",
  "repository": "wyze/wyze.github.io",
  "author": {
    "name": "Neil Kistner",
    "email": "neil.kistner@gmail.com",
    "url": "https://neilkistner.com"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn build:config",
    "build": "next build",
    "build:config": "rio -e next-offline,next-optimized-images,next-compose-plugins -o next.config.js next.config.ts",
    "predev": "yarn build:config",
    "dev": "next",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prestart": "yarn build:config",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@octokit/graphql": "^4.5.6",
    "next": "^11.1.0",
    "otion": "^0.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-otion": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.53",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "@wyze/rio": "^1.4.0",
    "babel-jest": "^26.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jest-dom": "^3.2.4",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-plugin-wyze": "^3.4.0",
    "jest": "^26.6.1",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.5",
    "next-optimized-images": "^2.6.2",
    "prettier": "^2.1.2",
    "typescript": "^4.3.5",
    "webp-loader": "^0.6.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!src/test-utils.tsx"
    ],
    "moduleNameMapper": {
      "\\.(css|pdf|png(\\?webp)?)$": "<rootDir>/__mocks__/file.ts"
    },
    "roots": [
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
